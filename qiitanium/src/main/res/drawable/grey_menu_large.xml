<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
  <item>
    <shape android:shape="oval">
      <size
          android:width="72dp"
          android:height="72dp"
          />
      <solid android:color="@color/grey"/>
    </shape>
  </item>
  <item
      android:bottom="8dp"
      android:left="8dp"
      android:right="8dp"
      android:top="8dp"
      >
    <shape android:shape="oval">
      <solid android:color="@color/grey"/>
      <stroke
          android:width="1dp"
          android:color="@color/divider"
          />
    </shape>
  </item>
</layer-list>